{
    "$id": "location.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Describes the location of a runtime object. Extension not allowed",
    "type": "object",
    "properties": {
        "gps": {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/gps.json"
        },
        "address": {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/address.json"
        },
        "station_code": {
            "type": "string"
        },
        "city": {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/city.json"
        },
        "country": {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/country.json"
        },
        "circle": {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/circle.json"
        },
        "polygon": {
            "type": "string"
        },
        "3dspace": {
            "type": "string"
        }
    }
}