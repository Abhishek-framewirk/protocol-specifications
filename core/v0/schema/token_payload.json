{
    "$id": "token_payload.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Describes the structure of the token payload",
    "type": "object",
    "properties" : {
        "domain" : {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/domain.json"
        },
        "country" : {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/country.json#/properties/code"
        },
        "city" : {
            "$ref": "https://raw.githubusercontent.com/beckn/protocol-specifications/core-0.9.0/core/v0/schema/city.json#/properties/code"
        },
        "ac_id" : {
            "type" : "string"
        },
        "key" : {
            "type" : "string"
        },
        "exp" : {
            "type" : "string",
            "format" : "date-time"
        }
    }
}