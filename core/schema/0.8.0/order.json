{
    "$id": "https://raw.githubusercontent.com/beckn/protocol-specifications/master/core/schema/0.8.0/order.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "version": "0.8.0",
    "description": "Describes the details of a service order",
    "type": "object",
    "properties": {
        "id" : {
            "type" : "string"
        },
        "state": {
            "type" : "string"
        },
        "created_at" : {
            "type" : "string",
            "format": "date-time"
        },
        "updated_at" : {
            "type" : "string",
            "format": "date-time"
        },
        "items" : {
            "type": "array",
            "items": {
                "$id": "https://raw.githubusercontent.com/beckn/protocol-specifications/master/core/schema/0.8.0/item.json#/properties/id"
            }
        }
    }
}