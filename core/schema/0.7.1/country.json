{
    "$id": "https://raw.githubusercontent.com/beckn/protocol-specifications/master/core/schema/0.7.1/country.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "version" : "0.7.1",
    "description": "indicates country code. Beckn supports country codes as per ISO 3166-1 standard",
    "type": "object",
    "properties" : {
        "standard" : {
            "type" : "string",
            "enum" : ["ISO 3166-1 ALPHA-2", "ISO 3166-1 ALPHA-3", "ISO 3166-1 NUMERIC)"]
        },
        "code" : {
            "type" : "string"
        }
    }
}