{
    "$id": "http://schema.beckn.org/core/schema/0.7.1/schedule.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "version" : "0.7.1",
    "description": "Describes schedule",
    "type": "object",
    "items" : {
        "day" : {
            "type" : "string",
            "enum" : ["MON", "TUE", "WED", "THU", "FRI", "SAT", "SUN"]
        },
        "slots" : {
            "type" : "array",
            "items" : {
                "open" : {
                    "$ref" : "https://schema.beckn.org/core/schema/0.7.1/time.json"
                },
                "close" : {
                    "$ref" : "https://schema.beckn.org/core/schema/0.7.1/time.json"
                }
            }
        }
    }
}